{
    "manifest_version": 3,
    "name": "Chat GPT on every site",
    "version": "1.0",
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["index.js"]
    }],
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png"
    },
    "permissions": ["activeTab","storage"],
    "options_page":"options/index.html"
}